{
  "expo": {
    "name": "Pokemon Guesser Native",
    "slug": "Pokemon-Guesser-Native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/poke-ball.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/poke-ball.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/poke-ball.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.RedDeadDepresso.PokemonGuesserNative"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/poke-ball.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "baseUrl": "/Pokemon-Guesser-Native"
    },
    "extra": {
      "eas": {
        "projectId": "93fd9db9-4206-4e91-9e44-b6cbc3cca825"
      }
    }
  }
}
